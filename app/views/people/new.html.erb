<h2>New person</h2>
  <%= form_for @person do |p| %>
    <%= p.label :name %>
    <%= p.text_field :name %>

    <%= p.label :age %>
    <%= p.number_field :age %>

    <%= p.label :hair_color %>
    <%= p.text_field :hair_color %>

    <%= p.label :race %>
    <%= p.text_field :race %>

    <%= p.submit %>
  <% end %>
